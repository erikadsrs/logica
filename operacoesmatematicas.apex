public class HandsOn1 {
    public static Integer somarValores(Integer n1, Integer n2){
        return n1+n2;
    }

    public static Integer subtrairValores(Integer n1, Integer n2){
        return n1-n2;
    }

    public static Integer multiplicarValores(Integer n1, Integer n2){
        return n1*n2;
    }

    public static Integer dividirValores(Integer n1, Integer n2){
        return n1/n2;
    }

    public static String concatenarTexto(String n1,String n2){
        return n1 + n2;
    }

    public static Integer tamanhoString (String exemplo){
        return exemplo.length();
    }
}
